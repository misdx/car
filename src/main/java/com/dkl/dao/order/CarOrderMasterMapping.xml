<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.dkl.dao.order.CarOrderMasterMapping">
    <!-- 创建订单 int carOrderMasterAdd(@Param("master") CarOrderMaster carOrderMaster); -->
    <insert id="carOrderMasterAdd">
        insert into carordermaster values(#{master.carOrderId},#{master.carUserName},#{master.carUserPhone},#{master.carUserId},#{master.carOrderAmount},default,default,default,default,default)
    </insert>
</mapper>